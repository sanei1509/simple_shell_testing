#include <stdio.h>
#include <string.h>

/**
 *main - splits a string a and return an array of the words
 */
void main(void)
{
	char string[50] = "Hello world we al ls ls";
	char *token;

	token = strtok(string, " ");

	while (token != NULL)
	{
		printf("%s\n", token);
		token = strtok(NULL, " ");
	}
}
